{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
    "module": {
      "type": "es6",
      "strict": true,
      "strictMode": true,
      "noInterop": false,
      "resolveFully": true
  },
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "target": "esnext",
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "keepClassNames": true,
    "baseUrl": "./"
  },
  "minify": false
}
